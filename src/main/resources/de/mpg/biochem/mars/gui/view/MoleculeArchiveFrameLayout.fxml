<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane stylesheets="@MoleculeArchiveFrame.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mpg.biochem.mars.gui.view.MoleculeArchiveFrameController">
   <children>
      <JFXTabPane fx:id="tabContainer" prefHeight="128.0" prefWidth="308.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
         	<Tab fx:id="propertiesTab" closable="false" text="Properties">
              <content>
                <AnchorPane fx:id="propertiesContainer"  minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab fx:id="imageMetaDataTab" closable="false" text="ImageMetaData">
              <content>
                <AnchorPane fx:id="imageMetaDataContainer"  minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab fx:id="moleculesTab" closable="false" text="Molecules">
              <content>
                <AnchorPane fx:id="moleculesContainer"  minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
         </tabs>
      </JFXTabPane>
   </children>
</StackPane>
