<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mpg.biochem.mars.gui.dialog.SinglePlotDialogController">
   <children>
      <GridPane alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="145.0" prefWidth="272.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="121.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="151.0" minWidth="10.0" prefWidth="151.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="36.0" minHeight="10.0" prefHeight="36.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="36.0" minHeight="10.0" prefHeight="36.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="36.0" prefWidth="136.0" text="x column" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="36.0" prefWidth="136.0" text="y column" GridPane.rowIndex="1" />
            <ComboBox prefWidth="150.0" GridPane.columnIndex="1" fx:id="xColumn" />
            <ComboBox fx:id="yColumn" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="124.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="27.0" prefWidth="77.0" text="OK" />
      <Button layoutX="209.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="27.0" prefWidth="77.0" text="Cancel" />
   </children>
</AnchorPane>
